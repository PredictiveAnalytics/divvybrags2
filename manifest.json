{
  "manifest_version": 2,

  "name": "DivvyBrags",
  "description": "Liberate your bikeshare data.",
  "version": "1.0",

  "icons": { 
    "48": "icon48.png"
  },

  "content_scripts": [
    {
      "matches": ["https://divvybikes.com/account/trips"],
      "js": ["jquery.js", "highcharts.js", "background.js"],
      "css": ["background.css"]
    } 
  ],

  "web_accessible_resources": [
    "jquery.js",
    "highcharts.js",
    "background.js",
    "background.css",
    "ajax-loader.gif",
    "station_distances_withheader.csv"
  ],

  "permissions": [
    "https://maps.googleapis.com/maps/api/distancematrix/json"
  ]

}
